<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
    <script src="../lib/angular.js"></script>
</head>
<body ng-app="myApp">
<di></di>
<div di=""></div>
<div class="di"></div>

<script>
    var app = angular.module("myApp",[]);
    app.directive("di",function(){
        return {
          replace:true,//是否显示当前自定义元素的标签
          restrict : "EAC",//(E 作为元素名使用  A 作为属性使用  C 作为类名使用  M 作为注释使用)
          template:'<h1>自定义的指令</h1>'
        };
    })
</script>
</body>
</html>