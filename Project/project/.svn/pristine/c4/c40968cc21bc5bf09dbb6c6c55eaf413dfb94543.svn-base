<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>循环输出两个结构数据</title>
  </head>
  <body ng-app="myApp" ng-controller="main as mm">
    <table>
      <tr ng-repeat-start="item in mm.nodes">
        <td>{{item.label}}</td>
      </tr>
      <tr ng-repeat-end>
        <td>{{item.id}}</td>
      </tr>
    </table>

    <script src="http://apps.bdimg.com/libs/angular.js/1.4.6/angular.js"></script>
    <script type="text/javascript">
      angular.module('myApp',[])
        .controller('main',[function(){
          this.nodes=[
            {
              id:1,
              label:'First Node',
              done:false
            },
            {
              id:3,
              label:'Finished Third Note',
              done:true
            },
            {
              id:2,
              label:'Second Note',
              done:false
            }
          ]
        }]);
    </script>
  </body>
</html>
