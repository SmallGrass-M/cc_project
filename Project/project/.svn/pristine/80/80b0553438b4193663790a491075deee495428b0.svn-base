<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
</head>
<body>

<script>
    function A(p){
        this.p = new Array();
        this.p.push(p);
    }
    A.prototype.logg = function(){
        for(var item of this.p){
            console.log(item);
        }
    }
    function B(p1,p2){
        this.p = new Array();
        this.p.push(p1);
        this.p.push(p2);
    }
    B.prototype= A.prototype;

    var a = new A("blue");
    a.logg();
    var b = new B("red","john");
    b.logg();
</script>
</body>
</html>